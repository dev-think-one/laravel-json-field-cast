{
    "name": "think.studio/laravel-json-field-cast",
    "description": "Cast json field to custom object.",
    "keywords": [
        "laravel",
        "json",
        "field",
        "cast"
    ],
    "homepage": "https://github.com/dev-think-one/laravel-json-field-cast",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yg@think.studio",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "orchestra/testbench": "^8.5|^9.0|^10.0",
        "phpunit/phpunit": "^10.2",
        "psalm/plugin-laravel": "^2.8",
        "vimeo/psalm": "^5.13"
    },
    "autoload": {
        "psr-4": {
            "JsonFieldCast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonFieldCast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
