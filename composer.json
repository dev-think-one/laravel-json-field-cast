{
    "name": "yaroslawww/laravel-json-field-cast",
    "description": "Cast json field to object.",
    "keywords": [
        "laravel",
        "json",
        "field",
        "cast"
    ],
    "homepage": "https://github.com/yaroslawww/laravel-json-field-cast",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "orchestra/testbench": "^6.21",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "JsonFieldCast\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonFieldCast\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
